<view class="content"><view class="header"><image src="../../static/shilu-login/logo.png"></image></view><view class="list"><view class="list-call"><image class="img" src="/static/shilu-login/1.png"></image><input class="sl-input" placeholder="序列号" data-event-opts="{{[['input',[['__set_model',['$0','sn','$event',[]],['userInfo']]]]]}}" value="{{userInfo.sn}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/shilu-login/2.png"></image><input class="sl-input" type="text" maxlength="32" placeholder="分店号" password="{{!showPassword}}" data-event-opts="{{[['input',[['__set_model',['$0','fdbh','$event',[]],['userInfo']]]]]}}" value="{{userInfo.fdbh}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/shilu-login/3.png"></image><input class="sl-input" placeholder="分店类型" data-event-opts="{{[['input',[['__set_model',['$0','fdlx','$event',[]],['userInfo']]]]]}}" value="{{userInfo.fdlx}}" bindinput="__e"/></view><view class="list-call"><image class="img" src="/static/shilu-login/4.png"></image><input class="sl-input" type="text" placeholder="用户工号" data-event-opts="{{[['input',[['__set_model',['$0','userid','$event',[]],['userInfo']]]]]}}" value="{{userInfo.userid}}" bindinput="__e"/></view></view><view class="button-login" hover-class="button-hover" data-event-opts="{{[['~tap',[['scan']]]]}}" bindtap="__e"><text>扫码注册</text></view><view class="agreement"><image src="{{agreement==true?'/static/shilu-login/ty1.png':'/static/shilu-login/ty0.png'}}" data-event-opts="{{[['tap',[['agreementSuccess',['$event']]]]]}}" bindtap="__e"></image><text data-event-opts="{{[['tap',[['agreementSuccess',['$event']]]]]}}" bindtap="__e">同意</text><navigator url="agreement?id=1" open-type="navigate">《软件用户协议》</navigator></view></view>